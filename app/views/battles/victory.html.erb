<div class="end-battle-box">
  <p class="gp-earned"><%= @battle.battle_level.gp_reward %></p>
  <p class="exp-earned"><%= @battle.battle_level.exp_given %></p>
  <% if @ability != nil %>
    <p class="ability-earned" data-class="<%= @class_list %>" data-slot= "<%= @slot %>">
      <%= @ability.name %>
    </p>
  <% else %>
    <p class="ability-earned">
    </p>
  <% end %>
  <%= link_to "Equip Ability", home_index_path, class: "end-battle-but ability-equip battle-fin" %>
  <%= link_to "Battle", new_battle_path, class: "end-battle-but level-select battle-fin" %>
</div>
