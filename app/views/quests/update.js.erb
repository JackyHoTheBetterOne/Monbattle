<% if @quest.save %>
  $("<%= dom_id(@quest) %>-edit").click()
  $("div#<%= dom_id(@quest) %>").html("<%= j render @quest %>").effect("highlight");
  setTimeout((function() {
    return $('body').scrollTo('#<%= dom_id(@quest) %>');
  }), 500);
<% else %>
  $("#<%= dom_id(@quest) %>-edit").effect("highlight", {color: "red"});
<% end %>