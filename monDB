--
-- PostgreSQL database dump
--

SET statement_timeout = 0;
SET lock_timeout = 0;
SET client_encoding = 'UTF8';
SET standard_conforming_strings = on;
SET check_function_bodies = false;
SET client_min_messages = warning;

--
-- Name: plpgsql; Type: EXTENSION; Schema: -; Owner: 
--

CREATE EXTENSION IF NOT EXISTS plpgsql WITH SCHEMA pg_catalog;


--
-- Name: EXTENSION plpgsql; Type: COMMENT; Schema: -; Owner: 
--

COMMENT ON EXTENSION plpgsql IS 'PL/pgSQL procedural language';


--
-- Name: hstore; Type: EXTENSION; Schema: -; Owner: 
--

CREATE EXTENSION IF NOT EXISTS hstore WITH SCHEMA public;


--
-- Name: EXTENSION hstore; Type: COMMENT; Schema: -; Owner: 
--

COMMENT ON EXTENSION hstore IS 'data type for storing sets of (key, value) pairs';


SET search_path = public, pg_catalog;

SET default_tablespace = '';

SET default_with_oids = false;

--
-- Name: abil_sockets; Type: TABLE; Schema: public; Owner: jackyho; Tablespace: 
--

CREATE TABLE abil_sockets (
    id integer NOT NULL,
    socket_num integer,
    created_at timestamp without time zone,
    updated_at timestamp without time zone
);


ALTER TABLE public.abil_sockets OWNER TO jackyho;

--
-- Name: abil_sockets_id_seq; Type: SEQUENCE; Schema: public; Owner: jackyho
--

CREATE SEQUENCE abil_sockets_id_seq
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1;


ALTER TABLE public.abil_sockets_id_seq OWNER TO jackyho;

--
-- Name: abil_sockets_id_seq; Type: SEQUENCE OWNED BY; Schema: public; Owner: jackyho
--

ALTER SEQUENCE abil_sockets_id_seq OWNED BY abil_sockets.id;


--
-- Name: abilities; Type: TABLE; Schema: public; Owner: jackyho; Tablespace: 
--

CREATE TABLE abilities (
    id integer NOT NULL,
    name character varying(255),
    ap_cost integer,
    min_level integer,
    created_at timestamp without time zone,
    updated_at timestamp without time zone,
    description character varying(255),
    stat_change character varying(255) DEFAULT ''::character varying,
    target_id integer,
    element_id integer,
    stat_target_id integer,
    image_file_name character varying(255),
    image_content_type character varying(255),
    image_file_size integer,
    image_updated_at timestamp without time zone,
    abil_socket_id integer,
    portrait_file_name character varying(255),
    portrait_content_type character varying(255),
    portrait_file_size integer,
    portrait_updated_at timestamp without time zone,
    keywords text,
    rarity_id integer,
    mp_cost integer,
    gp_cost integer
);


ALTER TABLE public.abilities OWNER TO jackyho;

--
-- Name: abilities_id_seq; Type: SEQUENCE; Schema: public; Owner: jackyho
--

CREATE SEQUENCE abilities_id_seq
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1;


ALTER TABLE public.abilities_id_seq OWNER TO jackyho;

--
-- Name: abilities_id_seq; Type: SEQUENCE OWNED BY; Schema: public; Owner: jackyho
--

ALTER SEQUENCE abilities_id_seq OWNED BY abilities.id;


