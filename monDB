    created_at timestamp without time zone,
    updated_at timestamp without time zone,
    mp integer DEFAULT 0,
    gp integer DEFAULT 0,
    current_exp integer DEFAULT 0,
    vortex_key integer DEFAULT 0,
    wins integer DEFAULT 0,
    losses integer DEFAULT 0,
    starting_status hstore,
    ending_status hstore,
    completed_daily_quests text[] DEFAULT '{}'::text[],
    completed_weekly_quests text[] DEFAULT '{}'::text[],
    completed_quests text[] DEFAULT '{}'::text[],
    daily_battles text[] DEFAULT '{}'::text[],
    beaten_levels text[] DEFAULT '{}'::text[],
    recently_completed_quests text[] DEFAULT '{}'::text[],
    stamina integer DEFAULT 100,
    level integer DEFAULT 1,
    seconds_left_for_next_energy integer,
    last_update_for_energy timestamp without time zone DEFAULT '2014-12-08 22:10:30'::timestamp without time zone,
    completed_areas text[] DEFAULT '{}'::text[],
    completed_regions text[] DEFAULT '{}'::text[],
    recently_unlocked_level character varying(255) DEFAULT ''::character varying
);


